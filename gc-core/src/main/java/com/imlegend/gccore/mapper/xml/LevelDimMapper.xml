<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imlegend.gccore.mapper.LevelDimDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.imlegend.gccore.domain.LevelDim">
        <result column="level_start" property="levelStart" />
        <result column="level_end" property="levelEnd" />
        <result column="level_name" property="levelName" />
    </resultMap>

</mapper>
