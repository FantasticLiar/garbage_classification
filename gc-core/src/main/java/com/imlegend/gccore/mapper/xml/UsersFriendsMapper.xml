<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imlegend.gccore.mapper.UsersFriendsDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.imlegend.gccore.domain.UsersFriends">
        <result column="uuid" property="uuid" />
        <result column="username" property="username" />
        <result column="uuid_f" property="uuidF" />
        <result column="username_f" property="usernameF" />
    </resultMap>

</mapper>
